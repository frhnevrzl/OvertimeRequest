@{
    Layout = "~/Views/Shared/AdminLTE/_Layout2.cshtml";
}
<head>
    <!-- Isolated Version of Bootstrap, not needed if your site already uses Bootstrap -->
    <link rel="stylesheet" href="https://formden.com/static/cdn/bootstrap-iso.css" />
    <!-- Bootstrap Date-Picker Plugin -->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-datepicker/1.4.1/css/bootstrap-datepicker3.css" />
    <link rel="stylesheet" href="~/css/RequestOvertime.css">
</head>
<div>
    <p class="maintitle">Request Overtime Form</p>
</div>
<section class="timesheet-navigation">
    <div class="nav">
        <div class="container-fluid nopaddingmail">
            <div class="tabbable">
                <ul class="nav nav-tabs" data-tabs="tabs" id="myTab">
                    <li class="active"><a data-toggle="tab" href="#incoming">Current</a></li>
                    <li><a data-toggle="tab" href="#sentmsg">Previous</a></li>
                    <li><a data-toggle="tab" href="#sentmsg">Not Sent</a></li>
                    <li><a data-toggle="tab" href="#sentmsg">Wait for Accept</a></li>
                    <li><a data-toggle="tab" href="#sentmsg">Accepted</a></li>
                    <li><a data-toggle="tab" href="#sentmsg">Rejected</a></li>
                </ul>
                <div class="tab-content">
                    <div class="tab-pane active" id="incoming">
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>
<div class="container-fluid">
    <div class="col-md-9 text-right">
        <!-- Form code begins -->
        <form method="post">
            <div class="form-group">
                <!-- Date input -->
                <label class="control-label" for="date">Date</label>
                <input class="form-control" id="date" name="date" placeholder="MM/DD/YYY" type="text" />
            </div>
            <div class="form-group">
                <!-- Submit button -->
                <button type="button" class="btn btn-secondary mb-2 mt-2" data-toggle="modal" data-target="#modalAddRequest">Add Request</button>
            </div>
        </form>
        <!-- Form code ends -->
    </div>
</div>
@section scripts{
    <!--  jQuery -->
    <script type="text/javascript" src="https://code.jquery.com/jquery-1.11.3.min.js"></script>
    <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-datepicker/1.4.1/js/bootstrap-datepicker.min.js"></script>
    <script src="~/js/RequestOvertime.js" asp-append-version="true"></script>
}
